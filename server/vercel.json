{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": [
            "database/**",
            "routes/**",
            "controllers/**",
            "mid/**",
            "config.env"
          ]
        }
      }
    ],
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
    ,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
      }
    ]
  }